{
    "name": "going-on-a-trip extension",
    "description": "higher than cloud nine",
    "version": "1.0",
    "manifest_version": 3,

    "permissions": ["scripting", "activeTab", "tabs"],
    "host_permissions": ["<all_urls>"],

    "action": {
        "default_popup": "popup.html"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
